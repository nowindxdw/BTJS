                    <!-- ===== Second Tab ===== -->

                    <div class="tab-pane " id="profile">
                        <table class="table table-hover">
                            <caption><h2><%=title%></h2></caption>
                            <%if(data!="undefined"){%>

                            <% for (var d in data){%>
                            <% var obj = data[d]%>
                            <thead>
                            <tr>
                                <th></th>
                                <th>内容</th>
                                <th>链接</th>
                                <th></th>
                            </tr>
                            </thead>

                            <%var details = obj.details%>

                            <tbody>

                            <tr>
                                <td></td>
                                <td><%=obj.trackName%><%=obj.remark%><a class="btn-info" href="/track">点击更新</a></td>
                                <td><a href="<%=obj.trackUrl%>" target="_blank">点击跳转</a></td>
                                <td></td>
                            </tr>
                            <% for (var i in details){%>
                            <% var item = details[i]%>
                            <tr>
                                <td width="10%" ><%=item.listOrder%></td>
                                <td width="50%" ><%=item.trackResult%></td>
                                <td width="30%" ><a href="<%=item.resultUrl%>" target="_blank">点击跳转</a></td>
                                <td width="10%" >
                                    <%if (item.remark == "UNREAD"){ %>
                                    <a href="/update?id=<%=item.id%>&&remark=1"><i class="fa fa-eye"></i></a>
                                    <%}else{%>
                                    <a href="/update?id=<%=item.id%>&&remark=0"><i class="fa fa-eye-slash"></i></a>
                                    <%}%>
                                </td>
                            </tr>
                            <%}%>

                            </tbody>
